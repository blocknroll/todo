<h1>ToDo's</h1>

<% @lists.each do |list| %>

	<h2><%= link_to "#{list.name}", list_url(list) %></h2>
	<p><%= list.description %> - <%= link_to "edit", edit_list_url(list) %> | <%= link_to "delete", list_url(list), :method => :delete %></p>

<% end %>

